@{
    ViewData["Title"] = "Carrello";
}
<h1 class="text-center my-3 text-uppercase">Il tuo carrello</h1>
<div class="container d-flex justify-content-around ">
    <div class="row align-items-center">
        <div class="col-3">
            <img src="data:image/jpeg;base64," alt="Carrello Image" class="img-fluid" />
        </div>
        <div class="col-6 text-center">
            <p class="fw-bold text-uppercase">Nome prodotto</p>
            <p class="text-secondary"> Dettaglio prodotto</p>
            <p>Prezzo prodotto</p>
        </div>
        <div class="col-3">
            <label for="quantity" class="fw-bold text-uppercase">Quantità:</label>
            <select id="quantity" name="quantity">
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>
                <option value="5">5</option>
            </select>
        </div>



        @* secondo prodotto *@
        <div class="col-3">
            <img src="data:image/jpeg;base64," alt="Carrello Image" class="img-fluid" />
        </div>
        <div class="col-6 text-center">
            <p class="fw-bold text-uppercase">Nome prodotto</p>
            <p class="text-secondary"> Dettaglio prodotto</p>
            <p>Prezzo prodotto</p>
        </div>
        <div class="col-3">
            <label for="quantity" class="fw-bold text-uppercase">Quantità:</label>
            <select id="quantity" name="quantity">
                <option value="1">1</option>
                <option value="2">2</option>
                <option value="3">3</option>
                <option value="4">4</option>
                <option value="5">5</option>
            </select>
        </div>

    </div>







</div>

@* compra *@
<div class="container ">
    <h3 class="mt-3">TOTALE ORDINE</h3>
    <div class="d-flex gap-4 ">
        <div>
            <p class="fw-bold">Subtotale: </p>
        </div>
        <div>
            <p> 100 </p>
        </div>
    </div>

    <div class="d-flex gap-4 ">
        <div>
            <p class="fw-bold">Spedizione: </p>
        </div>
        <div>
            <p> 0 </p>
        </div>
    </div>

    <div class="d-flex gap-4 ">
        <div>
            <p class="fw-bold">Totale: </p>
        </div>
        <div>
            <p> 100 </p>
        </div>
    </div>
</div>